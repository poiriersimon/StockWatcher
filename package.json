{
    "name": "com.simonpoirier.stockwatcher.plugin",
    "version": "1.0.0",
    "description": "Stream Deck plugin for displaying stock quotes",
    "type": "module",
    "scripts": {
        "build": "rollup -c",
        "watch": "rollup -c -w --watch.onEnd=\"streamdeck restart com.simonpoirier.stockwatcher.plugin\""
    },
    "dependencies": {
        "@elgato/streamdeck": "^2.0.0",
        "dotenv": "^17.2.3"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^29.0.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/node": "^25.1.0",
        "rollup": "^4.9.6",
        "tslib": "^2.6.2",
        "typescript": "^5.3.3"
    }
}
